    "eslint-config-prettier": "6.*",
    "eslint-plugin-editorconfig": "2.*",
    "eslint-plugin-jest": "23.*",
    "eslint-plugin-prettier": "3.1.*",
    "jest": "25.*",
    "prettier": "1.19.*",
    "rimraf": "3.0.*",
